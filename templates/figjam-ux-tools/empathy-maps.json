    "facilitationScript": [
      {
        "step": 1,
        "instruction": "Introduce persona and empathy mapping concept",
        "duration": "5 minutes",
        "sectionId": "workshop-intro", 
        "materials": ["persona overview", "empathy map template"]
      },
      {
        "step": 2,
        "instruction": "Fill Thinks & Feels quadrant - internal world",
        "duration": "8 minutes",
        "sectionId": "empathy-quadrants",
        "materials": ["research data", "user quotes", "behavioral observations"]
      },
      {
        "step": 3,
        "instruction": "Complete Sees quadrant - external environment",
        "duration": "8 minutes", 
        "sectionId": "empathy-quadrants",
        "materials": ["environmental context", "market influences"]
      },
      {
        "step": 4,
        "instruction": "Document Says & Does - observable behavior",
        "duration": "8 minutes",
        "sectionId": "empathy-quadrants",
        "materials": ["interview transcripts", "observation notes"]
      },
      {
        "step": 5,
        "instruction": "Identify Pains and Gains - barriers and motivators",
        "duration": "10 minutes",
        "sectionId": "empathy-quadrants",
        "materials": ["pain point analysis", "goal identification"]
      },
      {
        "step": 6,
        "instruction": "Synthesize insights and identify opportunities",
        "duration": "10 minutes",
        "sectionId": "insights-synthesis",
        "materials": ["insight templates", "opportunity frameworks"]
      }
    ],
    "layoutHints": {
      "arrangement": "sectioned",
      "spacing": 30,
      "columns": 4,
      "totalWidth": 1900,
      "totalHeight": 1600
    }
  },
  "aiPrompts": {
    "empathyGeneration": "Generate empathy map content for {userPersona} in {context}. Create 4-6 items for each quadrant: Thinks & Feels (internal emotions), Sees (environment/influences), Says & Does (behavior/quotes), Pains (frustrations), Gains (desires).",
    "insightSynthesis": "Analyze empathy map data from {quadrantData} to identify 3-5 key insights about user motivations and 2-3 design opportunities that address core needs.",
    "personaAlignment": "Ensure empathy map content aligns with {personaData} and reflects authentic user research findings from {researchSource}."
  },
  "qualityChecks": [
    "Each quadrant has 4-6 meaningful items",
    "Content reflects real user research data",
    "Thinks & Feels captures internal emotional state",
    "Says & Does includes direct quotes and behaviors",
    "Pains and Gains are specific and actionable",
    "Insights lead to clear design opportunities"
  ]
