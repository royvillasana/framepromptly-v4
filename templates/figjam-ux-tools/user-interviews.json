    "facilitationScript": [
      {
        "step": 1,
        "instruction": "Set up interview environment and explain process",
        "duration": "5 minutes",
        "sectionId": "workshop-setup",
        "materials": ["recording device", "consent form", "participant info"]
      },
      {
        "step": 2,
        "instruction": "Ask background questions to understand context",
        "duration": "10 minutes", 
        "sectionId": "interview-guide",
        "materials": ["question guide", "note-taking area"]
      },
      {
        "step": 3,
        "instruction": "Dive into behavioral questions and current processes",
        "duration": "25 minutes",
        "sectionId": "notes-collection",
        "materials": ["follow-up prompts", "observation checklist"]
      },
      {
        "step": 4,
        "instruction": "Explore pain points and improvement ideas",
        "duration": "15 minutes",
        "sectionId": "notes-collection",
        "materials": ["probing questions", "scenario examples"]
      },
      {
        "step": 5,
        "instruction": "Synthesize key insights and identify themes",
        "duration": "10 minutes",
        "sectionId": "synthesis",
        "materials": ["insight templates", "theme clustering"]
      }
    ],
