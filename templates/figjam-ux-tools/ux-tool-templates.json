{
  "templateMapping": {
    "toolSpecificTemplates": {
      "User Interviews": {
        "templateFile": "user-interviews.json",
        "structure": "interview-session",
        "primarySections": ["setup", "questions", "notes", "synthesis"],
        "itemCount": "12-16",
        "mainElements": ["research-objectives", "question-categories", "interview-notes", "key-insights"]
      },
      "Empathy Maps": {
        "templateFile": "empathy-maps.json", 
        "structure": "four-quadrants",
        "primarySections": ["intro", "quadrants", "insights"],
        "itemCount": "14-18",
        "mainElements": ["user-center", "thinks-feels", "sees", "says-does", "pains-gains"]
      },
      "Brainstorming": {
        "templateFile": "brainstorming.json",
        "structure": "ideation-workshop", 
        "primarySections": ["rules", "crazy8s", "expansion", "voting"],
        "itemCount": "20-25",
        "mainElements": ["challenge-statement", "brainstorm-rules", "idea-generation", "clustering-voting"]
      },
      "Persona Creation": {
        "templateFile": "persona-creation.json",
        "structure": "persona-profile",
        "primarySections": ["demographics", "behaviors", "goals", "pain-points"],
        "itemCount": "10-14",
        "mainElements": ["persona-photo", "bio-details", "goals-motivations", "pain-points-frustrations"]
      },
      "Journey Mapping": {
        "templateFile": "journey-mapping.json",
        "structure": "timeline-stages",
        "primarySections": ["stages", "touchpoints", "emotions", "opportunities"],
        "itemCount": "16-22",
        "mainElements": ["journey-stages", "user-actions", "emotional-journey", "pain-points-opportunities"]
      },
      "Affinity Mapping": {
        "templateFile": "affinity-mapping.json",
        "structure": "clustering-synthesis", 
        "primarySections": ["data-points", "initial-grouping", "themes", "insights"],
        "itemCount": "18-25",
        "mainElements": ["raw-data", "affinity-clusters", "theme-headers", "synthesized-insights"]
      },
      "SWOT Analysis": {
        "templateFile": "swot-analysis.json",
        "structure": "four-quadrants-matrix",
        "primarySections": ["strengths", "weaknesses", "opportunities", "threats"],
        "itemCount": "12-16", 
        "mainElements": ["swot-matrix", "internal-factors", "external-factors", "strategic-actions"]
      },
      "Stakeholder Mapping": {
        "templateFile": "stakeholder-mapping.json",
        "structure": "influence-interest-matrix",
        "primarySections": ["stakeholder-identification", "mapping-matrix", "engagement-strategies"],
        "itemCount": "14-18",
        "mainElements": ["stakeholder-list", "influence-matrix", "engagement-quadrants", "action-plans"]
      }
    },
    "layoutPatterns": {
      "interview-session": {
        "arrangement": "sectioned-vertical",
        "sectionHeight": 350,
        "spacing": 50,
        "totalHeight": 2000
      },
      "four-quadrants": {
        "arrangement": "quadrant-centered",
        "centerElement": "user-circle",
        "quadrantSize": 400,
        "spacing": 100
      },
      "ideation-workshop": {
        "arrangement": "progressive-expansion",
        "flowDirection": "top-to-bottom",
        "expansionFactor": 1.3,
        "voteConcentration": "bottom-section"
      },
      "timeline-stages": {
        "arrangement": "horizontal-timeline", 
        "stageWidth": 300,
        "timelineHeight": 600,
        "emotionalOverlay": true
      },
      "clustering-synthesis": {
        "arrangement": "organic-clustering",
        "clusterSpacing": 150,
        "hierarchicalLevels": 3,
        "synthesisArea": "right-side"
      }
    }
  },
  "contentGeneration": {
    "itemDistribution": {
      "User Interviews": {
        "objectives": 2,
        "questions": "4-6", 
        "notes": "6-8",
        "insights": "2-3"
      },
      "Empathy Maps": {
        "thinks-feels": "3-4",
        "sees": "3-4", 
        "says-does": "3-4",
        "pains": "2-3",
        "gains": "2-3"
      },
      "Brainstorming": {
        "rules": 4,
        "crazy8s": 8,
        "expansion": "8-12",
        "voting": "3-5"
      }
    },
    "colorSchemes": {
      "research-tools": {
        "primary": "#2196F3",
        "secondary": "#E3F2FD",
        "accent": "#1976D2",
        "text": "#0D47A1"
      },
      "analysis-tools": {
        "primary": "#9C27B0", 
        "secondary": "#F3E5F5",
        "accent": "#7B1FA2",
        "text": "#4A148C"
      },
      "ideation-tools": {
        "primary": "#4CAF50",
        "secondary": "#E8F5E8",
        "accent": "#388E3C",
        "text": "#1B5E20"
      }
    }
  },
  "validationRules": {
    "minimumItems": {
      "User Interviews": 12,
      "Empathy Maps": 14,
      "Brainstorming": 20,
      "Persona Creation": 10,
      "Journey Mapping": 16,
      "Affinity Mapping": 18,
      "SWOT Analysis": 12,
      "Stakeholder Mapping": 14
    },
    "requiredSections": {
      "User Interviews": ["workshop-setup", "interview-guide", "notes-collection", "synthesis"],
      "Empathy Maps": ["workshop-intro", "empathy-quadrants", "insights-synthesis"], 
      "Brainstorming": ["workshop-setup", "crazy-eights", "idea-expansion", "clustering-voting"]
    },
    "structuralElements": {
      "User Interviews": ["title", "objectives", "questions", "notes", "insights"],
      "Empathy Maps": ["user-center", "quadrants", "synthesis"],
      "Brainstorming": ["challenge", "rules", "ideas", "voting"]
    }
  }
}