  "facilitationScript": [
    {
      "phase": "Setup",
      "duration": "5 minutes",
      "activities": [
        "Welcome participants and introduce workshop goals",
        "Share FigJam board and explain navigation",
        "Set ground rules: stay in empathy mode, no solutions yet"
      ]
    },
    {
      "phase": "Icebreaker", 
      "duration": "10 minutes",
      "activities": [
        "Each person shares a user frustration on a sticky",
        "Quick clustering of similar frustrations",
        "Identify themes to keep in mind during workshop"
      ]
    },
    {
      "phase": "User Interview Synthesis",
      "duration": "30 minutes", 
      "activities": [
        "Review interview data and research findings",
        "Extract insights using Said-Felt-Did framework",
        "Focus on direct quotes and specific behaviors"
      ]
    },
    {
      "phase": "Empathy Mapping",
      "duration": "25 minutes",
      "activities": [
        "Fill out empathy map quadrants",
        "Focus on one primary user persona",
        "Look for contradictions and gaps in understanding"
      ]
    },
    {
      "phase": "Synthesis",
      "duration": "15 minutes",
      "activities": [
        "Identify key insights and patterns",
        "Prioritize most important findings",
        "Frame insights as opportunity statements"
      ]
    },
    {
      "phase": "Wrap-up",
      "duration": "5 minutes",
      "activities": [
        "Recap major discoveries",
        "Plan next steps and assign owners", 
        "Schedule Define phase workshop"
      ]
    }
  ],
  "aiPrompts": {
    "userInsightGeneration": "Generate realistic user insights for the {framework} {stage} phase focusing on {tool}. Create 6-8 insights that represent what users said, felt, and did during research. Each insight should be specific, actionable, and based on real user needs. Format as sticky note content (max 10 words each).",
    "empathyMapContent": "Create empathy map content for {userPersona} related to {problemArea}. Generate 4-6 items for each quadrant: Thinks & Feels, Sees, Says & Does, Pains, and Gains. Focus on authentic user emotions and behaviors observed during research.",
    "facilitationTips": "Provide facilitation guidance for running an empathize phase workshop with {participantCount} people for {duration} minutes. Include ice breakers, time management tips, and techniques for keeping the team focused on understanding rather than solving."
  },
  "tools": {
    "voting": {
      "enabled": true,
      "options": ["👍", "❤️", "💡", "🤔", "⚡"]
    },
    "timer": {
      "enabled": true,
      "sessions": ["5min", "10min", "15min", "25min"]
    },
    "reactions": {
      "enabled": true
    },
    "stamping": {
      "enabled": true,
      "purpose": "Priority voting on insights"
    }
  },
  "qualityChecks": [
    "Insights are specific and actionable",
    "Content focuses on understanding, not solutions",
    "User quotes are authentic and meaningful", 
    "Empathy map shows deep user understanding",
    "Next steps are clearly defined and assigned"
  ]
